{% extends 'base.html.twig' %}

{% block title %}Sign Up!{% endblock %}

{% block body %}
    <div class="form-sign-up m-auto w-100">
        {{ form_start(form) }}
        {{ form_widget(form) }}

        <button class="btn btn-lg btn-primary" type="submit">Sign up</button>
        <a class="btn btn-lg" href="{{ path('app_login') }}">Sign in</a>

        {{ form_end(form) }}
    </div>
{% endblock %}
